<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props" Condition="Exists('$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props')" />
  <PropertyGroup>
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
    <Platform Condition=" '$(Platform)' == '' ">AnyCPU</Platform>
    <ProjectGuid>{5CE469AB-8A05-489C-8BE7-FECDE89C55B6}</ProjectGuid>
    <OutputType>Library</OutputType>
    <AppDesignerFolder>Properties</AppDesignerFolder>
    <RootNamespace>dotacp.protocol</RootNamespace>
    <AssemblyName>dotacp.protocol</AssemblyName>
    <TargetFrameworkVersion>v4.7.2</TargetFrameworkVersion>
    <FileAlignment>512</FileAlignment>
    <Deterministic>true</Deterministic>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
    <OutputPath>bin\Debug\</OutputPath>
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
    <Optimize>true</Optimize>
    <OutputPath>bin\Release\</OutputPath>
    <DefineConstants>TRACE</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="JsonEnumMemberConverter.cs" />
    <Compile Include="JsonEnumValueAttribute.cs" />
    <Compile Include="Properties\AssemblyInfo.cs" />
    <Compile Include="TypeAliasConverter.cs" />
    <Compile Include="Schema.cs" />
    <Compile Include="Meta.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="schema\meta.json" />
    <None Include="schema\schema.json" />
    <None Include="schema\VERSION" />
    <None Include="scripts\gen_all.ps1" />
    <None Include="scripts\gen_meta.ps1" />
    <None Include="scripts\gen_schema.ps1" />
    <None Include="scripts\gen_schema.Tests.ps1" />
    <None Include="scripts\README.md" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="8.0.5" />
  </ItemGroup>
  <PropertyGroup>
    <SchemaHashFile>$(ProjectDir)obj\$(Configuration)\schema.hash</SchemaHashFile>
  </PropertyGroup>
  <Target Name="GenerateProtocolCode" BeforeTargets="BeforeBuild">
    <ItemGroup>
      <_SchemaFiles Include="schema\schema.json" />
      <_SchemaFiles Include="schema\meta.json" />
      <_SchemaFiles Include="schema\VERSION" />
      <_GeneratedFiles Include="Schema.cs" />
      <_GeneratedFiles Include="Meta.cs" />
    </ItemGroup>
    <GetFileHash Files="@(_SchemaFiles)" Algorithm="SHA256">
      <Output TaskParameter="Items" ItemName="_SchemaFileHashes" />
    </GetFileHash>
    <PropertyGroup>
      <_SchemaHash>@(_SchemaFileHashes->'%(FileHash)', '|')</_SchemaHash>
    </PropertyGroup>
    <ReadLinesFromFile File="$(SchemaHashFile)" Condition="Exists('$(SchemaHashFile)')">
      <Output TaskParameter="Lines" ItemName="_ExistingSchemaHashLines" />
    </ReadLinesFromFile>
    <PropertyGroup>
      <_ExistingSchemaHash>$([System.String]::Join("", %(_ExistingSchemaHashLines.Identity)))</_ExistingSchemaHash>
      <_NeedGen>false</_NeedGen>
      <_NeedGen Condition="!Exists('$(SchemaHashFile)')">true</_NeedGen>
      <_NeedGen Condition="'$(_ExistingSchemaHash)' != '$(_SchemaHash)'">true</_NeedGen>
    </PropertyGroup>
    <ItemGroup>
      <_MissingGeneratedFiles Include="@(_GeneratedFiles)" Condition="!Exists('%(Identity)')" />
    </ItemGroup>
    <PropertyGroup>
      <_NeedGen Condition="'@(_MissingGeneratedFiles)' != ''">true</_NeedGen>
    </PropertyGroup>
    <Exec Condition="'$(_NeedGen)' == 'true'" WorkingDirectory="$(ProjectDir)" Command="powershell -NoProfile -ExecutionPolicy Bypass -File &quot;$(ProjectDir)scripts\gen_all.ps1&quot; -NoDownload" />
    <WriteLinesToFile Condition="'$(_NeedGen)' == 'true'" File="$(SchemaHashFile)" Lines="$(_SchemaHash)" Overwrite="true" />
    <ItemGroup>
      <FileWrites Include="$(SchemaHashFile)" Condition="Exists('$(SchemaHashFile)')" />
    </ItemGroup>
  </Target>
  <Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />
</Project>